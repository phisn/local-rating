{
  "bindings": [
    {
      "name": "req",
      "type": "httpTrigger",
      "direction": "in",

      "authLevel": "anonymous"
    },
    {
      "name": "res",
      "type": "http",
      "direction": "out"
    },
    {
      "name": "connection",
      "type": "webPubSubConnection",
      "direction": "in",

      "hub": "socket",
      "userId": "{query.userid}"
    }
  ]
}
