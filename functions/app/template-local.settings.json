{
  "comment": "Rename to local.settings.json and fill <connection string>'s",

  "IsEncrypted": false,
  "Values": {
      "AzureWebJobsStorage": "UseDevelopmentStorage=true",
      "WebPubSubConnectionString": "<connection-string>",
      "FUNCTIONS_WORKER_RUNTIME": "node",
      "WebPubSubHub": "distribution",
      
      "_comment": "https://stackoverflow.com/questions/67728499/azure-functions-there-was-an-error-performing-a-read-operation-on-the-blob-stor",
      "AzureWebJobsSecretStorageType": "files"
  },
  "Host": {
      "LocalHttpPort": 7071,
      "CORS": "*"
  }
}
